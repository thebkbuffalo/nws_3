<p id="notice"><%= notice %></p>

<h1>Listing Water Testing Services</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @water_testing_services.each do |water_testing_service| %>
      <tr>
        <td><%= link_to 'Show', water_testing_service %></td>
        <td><%= link_to 'Edit', edit_water_testing_service_path(water_testing_service) %></td>
        <td><%= link_to 'Destroy', water_testing_service, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

