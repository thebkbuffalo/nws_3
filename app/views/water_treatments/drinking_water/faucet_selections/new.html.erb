<h1>Add a new picture</h1>
<%= form_for FaucetSelection.new, url: '/water_treatments/drinking_water/faucet_selections', html: { multipart: true } do |f| %>
  <div class="field">
    <%= f.label 'New Picture' %><br>
    <%= f.file_field :faucet_pic %>
  </div>
  <div class="field">
    <%= f.label 'New Tag' %><br>
    <%= f.text_field :faucet_tag %>
  </div>
  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
