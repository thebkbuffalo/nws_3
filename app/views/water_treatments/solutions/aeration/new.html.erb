<h1>add a new photo</h1>
<%= form_for Aeration.new, url: '/water_treatments/solutions/aeration', html: { multipart: true } do |f| %>
  <div class="field">
    <%= f.label 'New Picture' %><br>
    <%= f.file_field :aeration_pic %>
  </div>
  <div class="field">
    <%= f.label 'New Tag' %><br>
    <%= f.text_field :aeration_tag %>
  </div>
  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
