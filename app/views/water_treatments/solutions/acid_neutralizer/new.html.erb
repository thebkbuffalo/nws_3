<h1>Add a new picture</h1>
<%= form_for AcidNeutralizer.new, url: '/water_treatments/solutions/acid_neutralizer', html: { multipart: true } do |f| %>
  <div class="field">
    <%= f.label 'New Picture' %><br>
    <%= f.file_field :acid_pic %>
  </div>
  <div class="field">
    <%= f.label 'New Tag' %><br>
    <%= f.text_field :acid_tag %>
  </div>
  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
