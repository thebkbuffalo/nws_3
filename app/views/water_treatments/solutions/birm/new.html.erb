<h1>Add a new photo</h1>
<%= form_for Birm.new, url: '/water_treatments/solutions/birm', html: { multipart: true } do |f| %>
  <div class="field">
    <%= f.label 'New Picture' %><br>
    <%= f.file_field :birm_pic %>
  </div>
  <div class="field">
    <%= f.label 'New Tag' %><br>
    <%= f.text_field :birm_tag %>
  </div>
  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
