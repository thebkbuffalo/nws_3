<h1>Add a new photo</h1>
<%= form_for KxPro.new, url: '/water_treatments/solutions/kx_pro', html: { multipart: true } do |f| %>
  <div class="field">
    <%= f.label 'New Picture' %><br>
    <%= f.file_field :kx_pic %>
  </div>
  <div class="field">
    <%= f.label 'New Tag' %><br>
    <%= f.text_field :kx_tag %>
  </div>
  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
