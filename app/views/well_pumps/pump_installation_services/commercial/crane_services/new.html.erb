<h1>Add a new Crane Service picture</h1>
<%= form_for CraneService.new, url: '/well_pumps/pump_installation_services/commercial/crane_services', html: { multipart: true } do |f| %>
  <div class="field">
    <%= f.label 'New Picture' %><br>
    <%= f.file_field :crane_pic %>
  </div>
  <div class="field">
    <%= f.label 'New Tag' %><br>
    <%= f.text_field :crane_tag %>
  </div>
  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
