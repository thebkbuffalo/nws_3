<h1>Add a new Picture to Pumps Tanks and Boosters</h1>
<%= form_for PumpsTanksBooster.new, url: '/well_pumps/pump_installation_services/commercial/pumps_tanks_boosters', html: { multipart: true } do |f| %>
  <div class="field">
    <%= f.label 'New Picture' %><br>
    <%= f.file_field :ptb_pic %>
  </div>
  <div class="field">
    <%= f.label 'New Tag' %><br>
    <%= f.text_field :ptb_tag %>
  </div>
  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
