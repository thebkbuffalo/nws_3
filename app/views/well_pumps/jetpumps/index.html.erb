<div class="text-center">
  <h1>Jet Pumps</h1>
  <% if current_user %>
    <h2><%= link_to 'New Jet Pump Picture', new_well_pumps_jetpump_path %></h2>
  <% end %>
  <% @jetpumps.each do |jp| %>
    <div class="images">
      <%= link_to image_tag(jp.jp_pic.url(:medium)), jp.jp_pic.url, title: jp.jp_tag %>
      <h4><%= jp.jp_tag %></h4>
      <% if current_user %>
        <h4><%= link_to 'DELETE', "/well_pumps/jetpumps/#{jp.id}", method: :delete, data: { confirm: 'Are you sure?' } %></h4>
      <% end %>
    </div>
  <% end %>
</div>
<br>
