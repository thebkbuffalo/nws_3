<h1>Jet Pumps</h1>
<% @jetpumps.each do |jp| %>
  <div class="inline">
    <%= image_tag(jp.jp_pic.url(:medium)) %>
    <h4><%= jp.jp_tag %></h4>
    <% if current_user %>
      <h4><%= link_to 'DELETE', "/well_pumps/jetpumps/#{jp.id}", method: :delete, data: { confirm: 'Are you sure?' } %></h4>
    <% end %>
  </div>
<% end %>
<br>
<%= link_to 'New Jet Pump Picture', new_well_pumps_jetpump_path %>
